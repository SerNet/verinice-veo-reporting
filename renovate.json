{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "bbUseDefaultReviewers": false,
  "reviewers": ["jk"],
  "automerge": true,
  "automergeType": "pr",
  "assignAutomerge": true,
  "packageRules": [
    {
      "matchPackagePatterns": ["^com.openhtmltopdf:"],
      "groupName": "OpenHTMLtoPDF"
    },
        {
    "matchPackagePatterns": ["^com.helger.font:"],
      "groupName": "ph-fonts"
    }
  ]
}
